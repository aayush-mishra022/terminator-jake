#include <stdio.h>
int main() {
int M1,M2,M3,AVG;

printf("enter the marks for subject 1: \n");
scanf("%d", &M1);
printf("enter the marks for subject 2: \n");
scanf("%d", &M2);
printf("enter the marks for subject 3: \n");
scanf("%d", &M3);

AVG=(M1+M2+M3)/3;

if(M1<33 || M2<33 || M3<33){
    printf("LOW INDIVIDUAL MARKS\n");
    printf("FAILED\n");
}

else if(AVG < 40){
    printf("LOW AVERAGE MARKS\n");
    printf("FAILED\n");
}

else{
    printf("THE AVERAGE OF THE 3 MARKS IS : %d\n", AVG);
    printf("PASSED\n");
}


    return 0;
}
